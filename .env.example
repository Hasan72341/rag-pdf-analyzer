# GenAI Development Environment Variables

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORGANIZATION=your_org_id_here

# Anthropic Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Cohere API
COHERE_API_KEY=your_cohere_api_key_here

# Hugging Face Hub
HUGGINGFACE_HUB_TOKEN=your_huggingface_token_here

# Weights & Biases
WANDB_API_KEY=your_wandb_api_key_here

# LangSmith (LangChain)
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=genai-dev-project

# Pinecone Vector Database
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here

# Database Configuration
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=genai_db
POSTGRES_USER=genai_user
POSTGRES_PASSWORD=genai_password

REDIS_HOST=redis
REDIS_PORT=6379

ELASTICSEARCH_HOST=elasticsearch
ELASTICSEARCH_PORT=9200

# Qdrant Vector Database
QDRANT_HOST=qdrant
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334

# Ollama Local LLM
OLLAMA_HOST=ollama
OLLAMA_PORT=11434
OLLAMA_BASE_URL=http://ollama:11434

# Development Settings
PYTHONPATH=/workspace
JUPYTER_TOKEN=genai-dev-token
CODE_SERVER_PASSWORD=genai-dev-password

# CUDA Settings (for GPU support)
CUDA_VISIBLE_DEVICES=all